{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "ui-library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "componentName": {
      "type": "string",
      "description": "Component name"
    },
    "directory": {
      "type": "string",
      "description": "directory"
    }
  },
  "required": ["name", "componentName", "directory"]
}
